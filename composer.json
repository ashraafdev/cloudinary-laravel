{
    "name": "ashraafdev/cloudinary-laravel",
    "description": "Cloudinary Adapter for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Ashraafdev\\CloudinaryLaravel\\": "src/",
            "Ashraafdev\\CloudinaryLaravel\\Exception\\": "exception/",
            "AshraafDev\\Tests\\CloudinaryLaravel\\": "test/",
            "AshraafDev\\Helpers\\": "helpers/"
        },
        "files": ["helpers/HelperFunctions.php"]
    },
    "authors": [
        {
            "name": "Achraf Hebboul"
        }
    ],
    "require": {
        "league/flysystem": "^3.0.0",
        "cloudinary/cloudinary_php": "^2.12",
        "symfony/dotenv": "^6.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.4",
        "phpunit/phpunit": "^10.5"
    }
}
